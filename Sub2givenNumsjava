// Program to return subraction value of user given two numbers
import java.util.Scanner;
public class Sub2givenNums 
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the first number: ");
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=subtwonums(a, b);
        System.out.println("The sub of two numbers: "+c);
        sc.close();
    }
    public static int subtwonums(int m, int n)
    {
        int s=m-n;
        return s;
    }
    
}
