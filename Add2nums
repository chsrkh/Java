public class Add2nums 
{
    public static void main(String[] args)
    {
        int a=10;
        int b=20;
        int c=addtwonums(a, b);
        System.out.println("The add of two numbers: "+c);
    }
    public static int addtwonums(int m, int n)
    {
        int s=m+n;
        return s;
    }
    
}
